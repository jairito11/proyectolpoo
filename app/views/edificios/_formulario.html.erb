	<table>
		<tr>
			<th><%= f.label :codigo %></th>
			<td>
				<%=f.hidden_field :id %>
				<%= f.text_field :codigo %>
				
			</td>
		</tr>
		<tr>
			<th><%= f.label :direccion %></th>
			<td><%= f.text_field :direccion %></td>
		</tr>
		<tr>
			<th><%= f.label :pagina %></th>
			<td><%= f.text_field :pagina %></td>
		</tr>
		<tr>
			<th><%= f.label :telefono %></th>
			<td><%= f.text_field :telefono %></td>
		</tr>
		<tr>
			<th><%= f.label 'Hora apertura' %></th>
			<td><%= f.text_field :horaAbre,placeholder: "HH:MM" %></td>
		</tr>
		<tr>
			<th><%= f.label 'Hora cierre' %></th>
			<td><%= f.text_field :horaCierre,placeholder: "HH:MM" %></td>
		</tr>
		<tr>
			<th><%= f.label 'Numero de salones' %></th>
			<td><%= f.text_field :numeroSalones %></td>
		</tr>
		<tr>
			<th><%= f.label 'Numero de cubiculos' %></th>
			<td><%= f.text_field :numeroCubiculos %></td>
		</tr>
		<tr>
			<th><%= f.label 'Proveedor' %></th>
			<td><%= f.select :proveedor_id, @proveedores.collect{|e| [e.codigo,e.id]}, {include_blank: "Selecciona uno."} %></td>
		</tr>			
		<tr>
			<th></th>
			<td><%= f.submit "Guardar"%></td>
		</tr>		
	</table>