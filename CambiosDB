sesiones


ready = ->	
	validator = $('#formulario_autores').validate
		rules:
			'autor[nombre]':
				required: true
				minlength: 13
		messages:
			'autor[nombre]':
				required: 'Nombre requerido'
				minlength: 'Minimo 13 Caracteres'
$(document).ready(ready)
$(document).on('turbolinks:load', ready)
